#include<stdio.h>


	int main(){

	int i,j,o=1,h=3;

	for(i=1; i<=4; i++){
		for(j=4; j>o; j--){
		printf("  ");
		}
	
		while(j!=0){

		printf(" %d", i);
		
		j--;
		}
		
		for(i=1; i<=4; i++){
			for(j=1; j<=o; j++)
			printf(" %d ", j);
		
				printf("\n");
			o++;
			}

		printf("\n");
	o++;
		
	}
}
